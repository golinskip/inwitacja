    <table class="table table-striped">
        <thead>
            <tr>
                <th>
                    <span class='required'>{{'eventConfig.form.parameter.name'|trans}}</span>
                </th>
                <th>
                    {{'eventConfig.form.parameter.description'|trans}}
                </th>
                <th>
                    <span class='required'>{{'eventConfig.form.parameter.type'|trans}}</span>
                </th>
            </tr>
        </thead>
        <tbody id="eventConfig-parameterTable" class="eventConfig-editableTable tbody-sortable" data-prototype="{% filter escape %}{% include 'PanelBundle:EventConfig:tabs/rowParameter.html.twig' with {'form': form.vars.prototype} %}{% endfilter %}">
            {% for parameter in form.children %}
                {% include 'PanelBundle:EventConfig:tabs/rowParameter.html.twig' with {'form': parameter} %}
            {% endfor %}
        </tbody>
    </table>
    {{ form_widget(form) }}
    <a id="eventConfig-addParameter" class="eventConfig-addRow" data-table="eventConfig-parameterTable" href="#">
        <i class="fa fa-plus"></i> {{'eventConfig.form.parameter.addParameter'|trans}}
    </a>